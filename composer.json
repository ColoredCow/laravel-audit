{
	"name": "coloredcow/laravel-audit",
	"description": "Audit logging for Laravel",
	"version": "2.1.0",
	"keywords": [
		"coloredcow",
		"laravel",
		"audit",
		"logs"
	],
	"homepage": "https://github.com/coloredcow/laravel-audit",
	"license": "GPL-3.0+",
	"repositories":[
		{
			"type": "git",
			"url": "https://github.com/coloredcow/laravel-audit.git"
		}
	],
	"authors":[
		{
			"name": "Nishanth KD",
			"email": "kd@coloredcow.com",
			"homepage": "http://coloredcow.com"
		}
	],
	"stability": "dev",
	"require":{
		"php" : "^7.2.5|^8.0",
		"laravel/framework": "^6.0|^7.0|^8.0"
	},
	"autoload":{
		"psr-4":{
			"ColoredCow\\LaravelAudit\\": "src"
		}
	},
	"extra": {
        "laravel": {
            "providers": [
                "ColoredCow\\LaravelAudit\\AuditServiceProvider"
            ],
            "aliases": {
                "AuditFacade": "ColoredCow\\LaravelGSuite\\Facades\\GSuiteUserService"
            }
        }
    }
}
